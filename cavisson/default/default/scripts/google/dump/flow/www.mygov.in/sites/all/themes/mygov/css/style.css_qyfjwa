.selectric {
  border-radius: 14px;
  background: #ffffff;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
  position: relative;
  overflow: hidden;
  min-width: 100px;
}
.selectric .label {
  font-family: "Open Sans", sans-serif;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 0 30px 0 35px;
  font-size: 0.84em;
  line-height: 30px;
  color: #444;
  height: 30px;
  user-select: none;
}
.selectric .label:before {
  content: "";
  background: url(../images/sprite-icon.png) -35px -530px no-repeat;
  width: 14px;
  height: 14px;
  float: left;
  position: absolute;
  left: 12px;
  top: 9px;
}
.selectric .button {
  display: block;
  position: absolute;
  right: 0;
  top: 0;
  width: 30px;
  height: 30px;
  line-height: 30px;
  background-color: #fff;
  text-align: center;
  font: 0/0 a;
  *font: 20px/30px Lucida Sans Unicode, Arial Unicode MS, Arial;
  color: #575757;
}
.selectric .button:after {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 0;
  height: 0;
  border: 5px solid transparent;
  border-top-color: #BBB;
  border-bottom: none;
}

.selectric-focus .selectric {
  border-color: #aaaaaa;
}

.selectric-hover .selectric {
  border-color: #c4c4c4;
}
.selectric-hover .selectric .label {
  color: #000;
}
.selectric-hover .selectric .label:before {
