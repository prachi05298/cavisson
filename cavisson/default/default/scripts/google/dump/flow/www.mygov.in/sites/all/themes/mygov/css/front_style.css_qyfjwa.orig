@keyframes animatetop {
  from {
    top: -300px;
    opacity: 0;
  }
  to {
    top: 0;
    opacity: 1;
  }
}
.box-left .wc {
  font-size: 1.571em;
  font-family: 'poppinsregular',sans-serif;
}

