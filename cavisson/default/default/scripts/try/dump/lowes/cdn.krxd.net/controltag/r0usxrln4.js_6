ment","priority":null,"template_replacement":true,"internal":true,"criteria":[],"collects_data":true},{"id":32302,"name":"Krux Lowe's.com Transaction DTC","content":"<script>\n\n(function(){\n \n  function fire_pixel(transactionParams){\n    Krux('require:http').pixel({\n      url: '//beacon.krxd.net/transaction.gif',\n      data: transactionParams\n    });\n  }\n \n  // use only if no transaction date present on the page\n  function getDate(){\n    var date = new Date();\n    var ret = date.getFullYear();\n    // format months with leading zeros\n    var m = (date.getMonth()+1).toString();\n    m = (m.length==1)?'0'+m:m;\n    var d = (date.getDate()).toString();\n    d = (d.length==1)?'0'+d:d;\n    // all together now\n    ret = ret +'-'+m+'-'+d;\n    return ret;\n  }\n \n  var transInfo = {};\n\n  // add in the publisher uuid\n  transInfo['_kpid'] = 'e30196c7-5053-416c-94a4-c1acfb09b619';\n \n  var dl =  Krux('scrape.javascript', 'digitalData') || {};\n  if(dl && dl.page && dl.page.pageName == \"order-confirmation\"){\n\n    var prds = dl.products || {};\n\n    var pids = [], \n        pqty = [], \n        pitm = [],\n        pmldId = [], \n        pbrnd = [],\n        pname = [],\n        pvendor = [],\n        pmodel = [],\n        punitPrice = [],\n        subTtl = 0;\n\n    for(i=0; i<prds.length; i++){\n        if(prds[i].productId && prds[i].productQuantity){\n          pids.push(prds[i].productId);\n          pqty.push(prds[i].productQuantity);\n        }\n\n        if(prds[i].productUnitPrice){\n          punitPrice.push(prds[i].productUnitPrice);\n          subTtl += ( (parseFloat(prds[i].productUnitPrice) || 0) * (parseInt(prds[i].productQuantity)) || 0);\n        }\n\n        if(prds[i].productName){\n          pname.push(prds[i].productName);\n        }\n\n        // Stored as item#, vendor#, and model#\n        if(prds[i].ivm && prds[i].ivm.length > 0){\n          var prdArr =  prds[i].ivm[0].split('-');\n\n          pitm.push(prdArr[0]);\n          pvendor.push(prdArr[1]);\n          pmodel.push(prdArr[2]);\n\n        }\n        //pmldId.push(prds[i].needVarHere; \n        //pbrnd .push(prds[i].needVarHere;\n    }\n\n    //Required atts\n    transInfo['lowes_trans_products.productId'] = (pids.join(\",\") || '');\n    transInfo['lowes_trans_products.productQuantity'] = (pqty.join(\",\") || '');\n    transInfo['lowes_trans_date'] = getDate();\n\n    //Extra atts\n    transInfo['lowes_trans_products.itemName'] = (pname.join(\",\") || '');\n    transInfo['lowes_trans_products.itemNumber'] = (pitm.join(\",\") || '');\n    transInfo['lowes_trans_products.vendorId'] = (pvendor.join(\",\") || '');\n    transInfo['lowes_trans_products.modelId'] = (pmodel.join(\",\") || '');\n\n    //transInfo['lowes_trans_products.brandName'] = (pbrnd.join(\",\") || '');\n    transInfo['lowes_trans_products.productUnitPrice'] = (punitPrice.join(\",\") || '');\n    transInfo['lowes_trans_.cart.subTotal'] = (subTtl.toFixed(2) || '');\n\n    // leave this part alone //\n \n    // now fire the pixel\n    fire_pixel(transInfo);\n  }\n})();\n\n</script>","target":null,"target_action":"append","timing":"onready","method":"document","priority":null,"template_replacement":true,"internal":true,"criteria":[],"collects_data":true},{"id":32892,"name":"Krux Segments to Partner","content":"<script>\n (function() {\n     var _kxStore = Krux('require:store');\n     _kxStore.set('segs_partner', _kxStore.get('segs'));\n     Krux('segments:watch', function(segs) {\n         setTimeout(function() {\n             _kxStore.set('segs_partner', _kxStore.get('segs'));\n         }, 1);\n\n     });\n })();\n</script>","target":null,"target_action":"append","timing":"onready","method":"document","priority":null,"template_replacement":true,"internal":true,"criteria":[],"collects_data":true},{"id":32365,"name":"Krux Datalayer Custom Prefixes V2 - All Sites","content":"<script>\n(function() {\n    /* Selective Attribute DataLayer Library Tag */\n    var _, allAttr, allowedList, attr, attributes, dataLayerIngester, dataObj,\n        isAllowed, keepCase, normalizeName, omitKeys, optimizeNames, pageAttr, prefix,\n        toSet, trim, userAttr, value,\n        hasProp = {}.hasOwnProperty;\n    _ = Krux('require:underscore');\n\n\n    dataLayerIngester = Krux('require:scrape').ingestDataLayer;\n\n    /* Safe copy of dataLayer object */\n    dataObj = Krux('scrape.js_global', 'digitalData');\n\n    /* String trimming helper function */\n    trim = function(attr) {\n        return (\"\" + attr).replace(/^\\s+|\\s+$/g, '');\n    };\n\n    /* Attribute configs */\n    /* 3_22 removed page.pageType,page.siteSectionTwo,page.siteSectionThree,page.siteSection,profile.user.CurrentStore.zip,profile.user.storeNumber */\n    /* 3_30 removed products.itemNumber,products.sellingPrice,products.brandName,products.productId,page.pageName, paidsrchclk_engine,paidsrchclk_account,paidsrchclk_keyword,paidsrchclk_campaign */\n    pageAttr = _.map('page.sysEnv,globalVars.channel,internalsearch'.split(','), trim);\n    userAttr = _.map(''.split(','), trim);\n\n    /* Create a array of attributes striping any empty strings */\n    allAttr = _.without(pageAttr.concat(userAttr), '');\n\n    /* Configuration settings */\n    prefix = 'lowes_';\n    if(dataObj && dataObj.page.pageType){\n        switch(dataObj.page.pageType){\n            case \"shopping-cart\":\n                prefix += \"cart_\";\n                break;\n            case \"checkout-login\":\n            case \"checkout\":\n            case \"review-and-pay\":\n                prefix += \"trans_\"\n                break;\n            case \"super-category\":\n            case \"category\":\n            case \"product-list\":\n                if(dataObj.page.siteSection == \"savings\"){\n                    prefix += \"savings_\";\n                }else{prefix += \"cat_\";}\n                break;\n            case \"brand-page\":\n                prefix += \"brand_\";\n                break;\n            case \"search-results\":\n                prefix += \"search_\";\n                break;\n            case \"installation_services\":\n                prefix += \"install_\";\n                break;\n            case \"product-display\":\n                prefix += \"pdp_\";\n                break;\n            case \"promotionslandingpage\":\n                switch(dataObj.page.siteSectionTwo){\n                    case \"consumer_credit_center\":\n                        prefix += \"credit_\";\n                        break;\n                    case \"savings\":\n                        prefix += \"savings_\";\n                        break;\n                }\n                break;\n            case \"lci\":\n                prefix += \"lci_\";\n                break;\n            default:\n                break;\n        }\n    }\n \n\n    keepCase = 'true' === 'true';\n    omitKeys = 'undefined'.split(',');\n\n    /* Function to varify if attribute should be used */\n    isAllowed = function(value, whitelist) {\n        var i, len, str, x;\n        str = \"\" + value;\n        if (!((value != null) && str.length > 0)) {\n            return false;\n        }\n        for (i = 0, len = whitelist.length; i < len; i++) {\n            x = whitelist[i];\n            if (value.match(x) != null) {\n                return true;\n            }\n        }\n        return false;\n    };\n\n    /* Function to standardise the attribute names */\n    normalizeName = function(name, optimize) {\n        if (optimize == null) {\n            optimize = false;\n        }\n        if (keepCase) {\n            return name;\n        }\n        return (\"\" + name).replace(/([A-Z])([A-Z]+)/g, function(full, first, rest) {\n            if (optimize === true) {\n                return \"\" + first + (rest.toLowerCase());\n            }\n            return \"\" + full;\n        }).replace(/_*([A-Z])/g, '_$1').replace(/^_/, '').toLowerCase();\n    };\n\n    /* Creates a pattern/replacement config for dataLayer tool to clean up names */\n    optimizeNames = function(names) {\n        var config, i, len, name;\n        config = [];\n        \n        if (!prefix.match(/^_$|null|undefined|false/)) {\n            config.push({\n                pattern: /((?:page|user)_attr_)/,\n                replacement: \"$1\" + prefix\n            });\n        }\n        return config;\n    };\n\n    /* Get a full list of attributes usting the dataLayer tool */\n    attributes = dataLayerIngester(dataObj, {\n        omitKeys: _.without(omitKeys.concat([/gtm\\./, /sessionid/i,\n            /\\.phpsessid$/i, /\\.sid$/i, /\\.zenid$/i,\n            /\\.requestid$/i\n        ]), ''),\n        omitValues: [/.*@.*(?:\\..*)+/, /gtm\\./, /^(https?:)?\\/\\/[^\\/]+/,\n            /^\\/[^\\/]+/, /.{255}/\n        ],\n        caseSensitive: keepCase,\n        useFullPath: 'true' === 'true',\n        useLastValue: 'false' === 'true',\n        customDelimited: [/./],\n        altDelimiter: ',',\n        userKeys: _.map(userAttr, function(exp) {\n            return new RegExp(\"(^|\\\\.)\" + exp + \"$\");\n        }),\n        convertAttrNames: optimizeNames(allAttr)\n    });\n\n    /* Only set Attributes in the allowed list */\n    allowedList = _.map(allAttr, function(name) {\n        return new RegExp(\"(_attr_|_attr_\" + prefix + \"|\\\\.)\" + (\n            normalizeName(name, true)) + \"$\");\n    });\n    toSet = {};\n    for (attr in attributes) {\n        if (!hasProp.call(attributes, attr)) continue;\n        value = attributes[attr];\n        if (isAllowed(attr, allowedList)) {\n            toSet[attr] = value;\n        }\n    }\n    Krux('set', toSet);\n\n   \n    //If a cat page and pageName == siteSection == channel don't collect (per Lowe's)\n   \n    if(prefix == \"lowes_cat_\" && (dataObj.page.pageName == dataObj.page.siteSection) &&  (dataObj.page.pageName == dataObj.globalVars.channel)){\n        Krux(\"set\", {\"page_attr_lowes_cat_page.siteSection\" : ''});\n        Krux(\"set\", {\"page_attr_lowes_cat_page.hiddenSiteSection\" : dataObj.page.siteSection});\n    }\n\n     //Still need a few at a \"global\" prefix level as well...\n    if(dataObj.page){\n        if(dataObj.page.pageType || dataObj.page.pageType){\n            Krux('set', {'page_attr_lowes_page.pageType': dataObj.page.pageType });\n        }\n        if(dataObj.page.sysEnv){\n            Krux('set', {'page_attr_lowes_page.sysEnv': dataObj.page.sysEnv});\n        }else{\n            Krux('set', {'page_attr_lowes_page.sysEnv': Krux(\"get\", \"page_attr_domain\")});\n        }\n    }\n    if(dataObj.globalVars){\n        if(dataObj.globalVars.channel){\n            Krux('set', {'page_attr_lowes_globalVars.channel': dataObj.globalVars.channel});\n        }\n    }\n    /* 3_22 removed if(dataObj.profile && dataObj.profile.user ){\n        Krux('set', {'user_attr_lowes_profile.user.storeNumber': dataObj.profile.user.storeNumber});\n        if(dataObj.profile.user.CurrentStore){\n            Krux('set', {'user_attr_lowes_profile.user.CurrentStore.zip': dataObj.profile.user.CurrentStore.zip });\n        }\n    }*/\n\n    //If not a prefixed page and siteSection, siteSectionTwo, and siteSectionThree don't collect (per Lowe's)\n    if(prefix == \"lowes_\" && (dataObj.page.siteSection == dataObj.page.siteSectionTwo) &&  (dataObj.page.siteSection == dataObj.page.siteSectionThree)){\n        Krux(\"set\", {\"page_attr_lowes_page.siteSection\" : ''});\n        Krux(\"set\", {\"page_attr_lowes_page.hiddenSiteSection\" : dataObj.page.siteSection});\n    }\n\n}).call();\n</script>","target":null,"target_action":"append","timing":"onload","method":"document","priority":null,"template_replacement":true,"internal":true,"criteria":[],"collects_data":true},{"id":32880,"name":"Krux DTC - CM MMC Click Param DTC","content":"<script>\n// Custom code to pull custom media click param values in from URL params for Lowes\n//Ex URL: https://www.lowes.com/?cm_mmc=display_-_-20042959-_-400538166-_-3381145-_-20056871390337718&2715359760\n\n(function(){\n\n\tvar params = Krux('scrape.url_param', 'cm_mmc');\n\t\n\tif(params){\n\t\tvar p = params.split('-_-');\n\t\tvar dataObj = {};\n\t\tvar param_type = \"paidsrchclk\";\n\t\tvar is_search = false;\n\t\tvar is_email = /^email_/.test(p[0] || \"\");\n\t\tvar is_social = /^sm(?!s)/.test(p[0] || \"\");\n\n\t\t//First check, first element is a \"search\" type value\n\t\tswitch((p[0] || '').toLowerCase()){\n\t\t\tcase 'yext':\n\t\t\tcase 'yext_pro':\n\t\t\t\tparam_type = 'naturalsrchclk';\n\t\t\t\tis_search = true;\n\t\t\t\tbreak;\n\t\t\tcase 'aff_cj':\n\t\t\t\tparam_type = 'affpaidsrchclk';\n\t\t\t\tis_search = true;\n\t\t\t\tbreak;\n\t\t\tcase 'sce_shopzillafeed':\n\t\t\tcase 'sce_shoppingfeed':\n\t\t\t\tparam_type = 'csesrchclk';\n\t\t\t\tis_search = true;\n\t\t\t\tbreak;\n\t\t}\n\t\t//Second check (if not found in first check), is the length of the values < = 4,\n\t\tif(!is_search && !is_email){\n\t\t\tis_search = p.length <= 4;\n\t\t}\n\n\t\t//If this is a search click, and the test for is a social click is false\n\t\tif( (is_search || is_email) && !is_social){ \n\t\t\t//paid search or email click.  4 sections\n\t\t\t//EX URL: https://www.lowes.com/c/Appliances?cm_mmc=search_google-_-Appliances-_-B_app_000_appliances_exact-_-lowes%20appliances&k_clickID=6139bc94-ae24-42a5-97a3-ae8da617a578&gclid=EAIaIQobChMIlfa6pLPe1QIVBYN-Ch3IFA1LEAAYASAAEgIV2PD_BwE\n\n\t\t\tif(is_email) { param_type = \"emailclk\"};\n\n\n\t\t\tdataObj['page_attr_lowes_' + param_type + '_engine'] =  (p[0]|| '');\n\t\t\tdataObj['page_attr_lowes_' + param_type + '_account'] = (p[1]|| '');\n\t\t\tdataObj['page_attr_lowes_' + param_type + '_campaign'] = (p[2]|| '');\n\t\t\tdataObj['page_attr_lowes_' + param_type + '_keyword'] = (p[3]|| '');\n\t\t}else{ \n\t\t\tparam_type = (is_social ? 'social_sc' : 'media_sc');\n\n\t\t\t//Media click. > 4 sections  Ex URL: https://www.lowes.com/?cm_mmc=display_-_-20042959-_-400538166-_-3381145-_